{
  "_version": "1.45.0",
  "sap.app": {
    "id": "com.pontual.sgmr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.7.4",
      "toolsId": "becbb6d9-0c68-440f-8e58-93480b7e7c93"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSGMR_MOBILE_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.pontual.sgmr.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "sgmrODataModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingModel": "TwoWay",
          "defaultCountMode": "Request"
        },
        "dataSource": "mainService",
        "preload": true
      },
      "conexaoModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "mensagensModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "loginModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "usuariosLoginModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "busyDialogModel": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.pontual.sgmr.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": ":?query:",
          "target": [
            "TargetApp"
          ]
        },
        {
          "name": "Configurar",
          "pattern": "Configurar",
          "target": [
            "Configurar"
          ]
        },
        {
          "name": "Login",
          "pattern": "Login",
          "target": [
            "Login"
          ]
        },
        {
          "name": "Inicio",
          "pattern": "Inicio",
          "target": [
            "Inicio"
          ]
        },
        {
          "name": "Administrativo",
          "pattern": "Administrativo",
          "target": [
            "Administrativo"
          ]
        }
      ],
      "targets": {
        "TargetApp": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "App",
          "viewName": "App"
        },
        "Inicio": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Inicio",
          "viewName": "Inicio"
        },
        "Login": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "Configurar": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Configurar",
          "viewName": "Configurar"
        },
         "Administrativo": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Administrativo",
          "viewName": "Administrativo"
        }
      }
    },
    "rootView": {
      "viewName": "com.pontual.sgmr.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}