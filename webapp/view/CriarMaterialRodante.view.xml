										value="{CriarMaterialRodanteModel>Centro}"
                                        maxLength="4"
										enabled="false"										
										showClearIcon="true"									
										/>					
								</f:fields>
							</f:FormElement>
							
							<f:FormElement label="Série">
								<f:fields>
									<Input value="{CriarMaterialRodanteModel>Autor}"
										   enabled="false" />
								</f:fields>
							</f:FormElement>		
							<f:FormElement label="Horímetro Eqto">
